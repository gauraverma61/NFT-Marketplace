{
  "compilerOptions": {
    "esModuleInterop": true,
    "module": "commonjs",  // Ensure module system is commonjs
    "target": "es6",
    "strict": true,
    "outDir": "./dist",
    "rootDir": "./src",
    "resolveJsonModule": true,
    "skipLibCheck": true
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}